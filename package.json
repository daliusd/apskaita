  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "start-with-port": "next start -p $PORT",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "jest --config=jest.config.js",
    "test:e2e": "playwright test",
    "test:docker": "DOCKER_HOST=\"unix://$HOME/.colima/docker.sock\" docker run -it --rm --ipc=host -v \"${PWD}:/var/app/\" mcr.microsoft.com/playwright:v1.31.2-focal /bin/bash -c 'cd /var/app; yarn; yarn playwright test'",
    "lint": "next lint",
    "migrate": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/migrate-dbs.ts",
    "prepare": "husky install .config/husky"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
