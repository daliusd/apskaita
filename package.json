    "prettier": "2.8.7",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}|pages/**/*.{ts,tsx}|tests/**/*.ts": [
      "eslint"
    ]
  },
  "packageManager": "yarn@3.5.0"
