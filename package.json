{
  "name": "nextjs",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "latest",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/pickers": "3.2.10",
    "@sentry/browser": "6.1.0",
    "@sentry/integrations": "6.1.0",
    "@sentry/node": "6.1.0",
    "@sentry/webpack-plugin": "1.14.0",
    "@types/pdfkit": "0.10.6",
    "@types/sharp": "0.27.1",
    "clsx": "latest",
    "date-fns": "2.17.0",
    "multer": "1.4.2",
    "next": "latest",
    "next-auth": "3.2.1",
    "pdfkit": "0.11.0",
    "react": "latest",
    "react-dom": "latest",
    "react-recipes": "1.1.1",
    "sharp": "0.27.1",
    "sqlite": "4.0.19",
    "sqlite3": "5.0.1",
    "swr": "0.4.2",
    "ts-node": "9.1.1",
    "uuid": "8.3.2"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --config=jest.config.js",
    "test:e2e": "jest --config=jest.e2e.config.js",
    "lint": "eslint --ext .ts,.tsx src/ && eslint --ext .ts,.tsx pages/ && eslint --ext ts tests/",
    "migrate": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/migrate-dbs.ts"
  },
  "devDependencies": {
    "@types/chance": "1.1.1",
    "@types/jest": "26.0.20",
    "@types/multer": "1.4.5",
    "@types/next-auth": "3.1.24",
    "@types/pixelmatch": "5.2.2",
    "@types/pngjs": "6.0.0",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "chance": "1.1.7",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-playwright-preset": "1.4.5",
    "lint-staged": "10.5.4",
    "pixelmatch": "5.2.1",
    "playwright": "1.8.0",
    "pngjs": "6.0.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.1",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}|pages/**/*.{ts,tsx}|tests/**/*.ts": [
      "eslint --max-warnings 0"
    ]
  }
}
