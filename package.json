{
  "name": "nextjs",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "latest",
    "clsx": "latest",
    "next": "latest",
    "next-auth": "3.1.0",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest",
    "swr": "0.3.6"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "test": "jest",
    "lint": "eslint --ext .js src/ && eslint --ext .js pages/ && eslint --ext js tests/"
  },
  "devDependencies": {
    "chance": "1.1.7",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "jest-puppeteer": "4.4.0",
    "lint-staged": "10.5.0",
    "pptr-testing-library": "0.6.4",
    "prettier": "2.1.2",
    "puppeteer": "5.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js|pages/**/*.js|tests/**/*.js": [
      "eslint --max-warnings 0"
    ]
  }
}
