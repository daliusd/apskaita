  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^2.16.0",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "latest",
    "@emotion/styled": "^11.10.5",
    "@googleapis/drive": "8.4.0",
    "@googleapis/gmail": "6.0.0",
    "@mui/base": "5.0.0-beta.22",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@mui/system": "^5.11.8",
    "@mui/x-date-pickers": "^6.0.0",
    "@types/number-to-words": "1.2.2",
    "@types/pdfkit": "0.13.1",
    "@types/sharp": "0.31.1",
    "date-fns": "^2.29.3",
    "googleapis-common": "7.0.0",
    "gray-matter": "4.0.3",
    "jimp": "^0.22.4",
    "multer": "1.4.4",
    "next": "^13.2.4",
    "next-auth": "4.24.4",
    "nodemailer": "6.9.7",
    "number-to-words": "1.2.4",
    "pdfkit": "0.13.0",
    "react": "latest",
    "react-dom": "latest",
    "react-use": "^17.4.0",
    "recharts": "^2.7.2",
    "recoil": "0.7.7",
    "remark": "14.0.3",
    "remark-html": "15.0.2",
    "sqlite": "5.0.1",
    "sqlite3": "5.1.6",
    "stacktrace-js": "2.0.2",
    "strip-markdown": "5.0.1",
    "swr": "2.2.4",
    "ts-node": "10.9.1",
    "uuid": "9.0.1"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "start-with-port": "next start -p $PORT",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "vitest run",
    "test:e2e": "yarn playwright test",
    "test:docker": "DOCKER_HOST=\"unix://$HOME/.colima/docker.sock\" docker run -it --rm --ipc=host -v \"${PWD}:/var/app/\" mcr.microsoft.com/playwright:v1.31.2-focal /bin/bash -c 'cd /var/app; yarn; yarn playwright test'",
    "lint": "next lint",
    "migrate": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/migrate-dbs.ts",
    "info": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/info.ts",
    "prepare": "husky install .config/husky"
  },
  "devDependencies": {
    "@playwright/test": "1.39.0",
    "@types/chance": "1.1.5",
    "@types/multer": "1.4.9",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "6.4.13",
    "chance": "1.1.11",
    "eslint": "latest",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "prop-types": "^15.8.1",
    "typescript": "5.2.2",
    "vitest": "^0.34.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}|pages/**/*.{ts,tsx}|tests/**/*.ts": [
      "eslint"
    ]
  },
  "packageManager": "yarn@3.6.4"
}
